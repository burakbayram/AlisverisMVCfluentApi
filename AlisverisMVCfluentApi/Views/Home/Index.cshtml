@using Entity
@model List<Urun>
@using (Html.BeginForm())
{
    if (Session["id"] != null)
    {
        foreach (var item in Model)
        {


            @Html.Label(item.UrunAdi)
            @Html.Label(item.Aciklama)
            @Html.Label(item.BirimFiyati.ToString())
            @Html.Editor("numeric", new { @type = "number", @min = "0", @step = "0.01", @value = "0" })


            <button class="btn btn-primary">Sepete Ekle</button>
        }

    }



}

